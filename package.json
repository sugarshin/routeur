{
  "name": "routeur",
  "version": "0.2.2",
  "description": "Super tiny clientside static url router",
  "repository": {
    "type": "git",
    "url": "git@github.com:sugarshin/routeur.git"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "package.json",
    "README.md",
    "test"
  ],
  "keywords": [
    "router",
    "route",
    "routing",
    "url"
  ],
  "scripts": {
    "lint": "eslint src",
    "start": "npm run compile -- -w",
    "test": "mocha --compilers js:espower-babel/guess test/**/*.js",
    "compile": "babel -d lib src",
    "standalone": "browserify src/routeur.js -t babelify -s Routeur -p licensify | derequire > build/routeur.js",
    "minify": "uglifyjs build/routeur.js -m -c warnings=false --comments '/This header is generated by licensify/' -o build/routeur.min.js",
    "build": "npm run lint && npm run compile && npm run standalone && npm run minify"
  },
  "author": "sugarshin",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "derequire": "^2.0.2",
    "eslint": "^1.5.1",
    "espower-babel": "^3.3.0",
    "licensify": "^2.0.1",
    "mocha": "^2.3.3",
    "power-assert": "^1.0.1",
    "uglify-js": "^2.4.24"
  },
  "dependencies": {
    "ext-regex": "^1.0.0",
    "glob-to-regexp": "^0.1.0",
    "object-assign": "^4.0.1",
    "object.omit": "^2.0.0"
  }
}
