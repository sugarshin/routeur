{
  "name": "routeur",
  "version": "0.0.1",
  "description": "Super tiny static url router",
  "repository": {
    "type": "git",
    "url": "git@github.com:sugarshin/routeur.git"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "package.json",
    "README.md",
    "test"
  ],
  "scripts": {
    "lint": "eslint src/",
    "test": "mocha --compilers js:espower-babel/guess test/**/*.js",
    "compile": "babel -d lib/ src/",
    "standalone": "browserify src/routeur.js -t babelify -s Routeur -p licensify | derequire > build/routeur.js",
    "minify": "uglifyjs build/routeur.js -m -c warnings=false --comments='This header is generated by licensify' -o build/routeur.min.js",
    "build": "npm run lint && npm run compile && npm run standalone && npm run minify"
  },
  "engines": {
    "node": "4.1.x"
  },
  "author": "sugarshin",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "derequire": "^2.0.2",
    "eslint": "^1.4.3",
    "espower-babel": "^3.3.0",
    "licensify": "^2.0.1",
    "mocha": "^2.3.2",
    "power-assert": "^1.0.1",
    "uglify-js": "^2.4.24"
  },
  "dependencies": {
    "object-assign": "^4.0.1",
    "object.omit": "^2.0.0",
    "path-to-regexp": "^1.2.1"
  }
}
