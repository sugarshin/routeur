    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18",
    "eslint": "^1.9.0",
    "eslint-config-sugarshin": "0.0.13",
    "eslint-plugin-babel": "^2.1.1",
    "espower-babel": "^3.3.0",
    "mocha": "^2.3.3",
    "power-assert": "^1.1.0"
  },
  "dependencies": {
    "ext-regex": "^1.0.0",
